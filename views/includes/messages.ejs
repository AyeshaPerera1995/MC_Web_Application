<% if(error!=''){ %>
    <script>
        showNotification('alert-danger', '<%= error %> ', 'top', 'right', 'Enter','Exit');
    </script>
<% } %>

<% if(success_msg!=''){ %>
    <script>
        showNotification('alert-success', '<%= success_msg %> ', 'top', 'right', 'Enter','Exit');
    </script>
<% } %>

<% if(error_msg!=''){ %>
    <script>
        showNotification('alert-danger', '<%= error_msg %>'+'   ', 'top', 'right', 'Enter','Exit');
    </script>
<% } %>


