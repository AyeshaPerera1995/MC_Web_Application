<script>
    function loadme(){

        if("<%= details.plan.webPortal %>"=="YES"){
            document.getElementById('HeadderPanel').style.display='block'; 
            document.getElementById('ClipUsagedetails').style.display='block'; 
            document.getElementById('DeviceUsage').style.display='block'; 
            document.getElementById('lockedpanel').style.display='none'; 
            document.getElementById('planDetails').style.display='none'; 
        
        }else if("<%= details.plan.webPortal %>"=="NO"){
            document.getElementById('HeadderPanel').style.display='none'; 
            document.getElementById('ClipUsagedetails').style.display='none'; 
            document.getElementById('DeviceUsage').style.display='none'; 
            document.getElementById('lockedpanel').style.display='block';
            document.getElementById('planDetails').style.display='block';  
        }
            
    } 

</script>


