                                    <% var pro = details.clipCountDetails.android_percentage; %>
                                <div class="progress-bar l-green" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: <%=pro%>%;"></div>
                                </div>
